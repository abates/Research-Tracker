<!DOCTYPE html>
<html>
<head>
  <title>Andrew's Thesis Work</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="head">
    <%= image_tag "scholar.png" %>
    <h1>Andrew's Research Organizer</h1>
  </div>
  <div id="body">
    <div id="content">
      <% unless notice.blank? %><p class="notice"><%= notice %></p><% end %>
      <%= yield %>
    </div>
    <div id="right_sidebar">
      <%= render :partial => 'layouts/navbar' %>
    </div>
    <div style="clear: both">&nbsp;</div>
  </div>
  <div id="foot">
  </div>
</body>
</html>
